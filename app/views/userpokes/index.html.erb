<p id="notice"><%= notice %></p>

<h1>Listing Userpokes</h1>

<table>
  <thead>
    <tr>
      <th>Pokemon</th>
      <th>User</th>
      <th>Cp</th>
      <th>Hp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userpokes.each do |userpoke| %>
      <tr>
        <td><%= userpoke.pokemon %></td>
        <td><%= userpoke.user %></td>
        <td><%= userpoke.cp %></td>
        <td><%= userpoke.hp %></td>
        <td><%= link_to 'Show', userpoke %></td>
        <td><%= link_to 'Edit', edit_userpoke_path(userpoke) %></td>
        <td><%= link_to 'Destroy', userpoke, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userpoke', new_userpoke_path %>
